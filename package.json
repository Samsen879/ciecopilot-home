{
  "name": "cie-copilot",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "migrate": "node scripts/migrate-data.js",
    "db:setup": "echo 'Please follow SUPABASE_SETUP.md for database setup'",
    "db:migrate": "node scripts/run-sql-migration.js",
    "rag:ingest:notes": "node scripts/rag_ingest.js --notes",
    "rag:ingest:pdf": "node scripts/rag_ingest.js --pdf",
    "rag:search:demo": "node scripts/rag_search_demo.js",
    "agent-b:setup": "node scripts/setup-agent-b.js",
    "agent-b:test": "node scripts/test-api-integration.js",
    "agent-b:dev": "npm run agent-b:test && npm run dev",
    "api:dev": "vercel dev",
    "api:test": "jest",
    "api:test:watch": "jest --watch",
    "api:test:coverage": "jest --coverage",
    "test:community": "node scripts/test-community-api.js",
    "test:community:dev": "npm run dev & sleep 5 && npm run test:community"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.53.1",
    "@tanstack/react-query": "^5.84.2",
    "@tanstack/react-virtual": "^3.13.12",
    "bcryptjs": "^2.4.3",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "fast-deep-equal": "^3.1.3",
    "framer-motion": "^12.18.1",
    "helmet": "^7.1.0",
    "i18next": "^25.3.2",
    "jsonwebtoken": "^9.0.2",
    "katex": "^0.16.22",
    "lodash": "^4.17.21",
    "lucide-react": "^0.516.0",
    "moment": "^2.29.4",
    "msw": "^2.10.4",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.7",
    "openai": "^5.12.0",
    "pdf-parse": "^1.1.1",
    "pdfjs-dist": "^5.4.54",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^15.6.1",
    "react-markdown": "^10.1.0",
    "react-pdf": "^10.0.1",
    "react-router-dom": "^6.30.1",
    "redis": "^4.6.10",
    "rehype-katex": "^7.0.1",
    "remark-math": "^6.0.0",
    "tailwind-merge": "^3.3.1",
    "uuid": "^9.0.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.8",
    "@vitejs/plugin-react": "^4.5.2",
    "autoprefixer": "^10.4.19",
    "dotenv": "^17.2.1",
    "eslint": "^8.54.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "jest": "^29.7.0",
    "node-fetch": "^3.3.2",
    "nodemon": "^3.0.1",
    "postcss": "^8.4.33",
    "supertest": "^6.3.3",
    "tailwindcss": "^3.4.1",
    "vite": "^5.0.12"
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  },
  "jest": {
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "api/**/*.js",
      "!api/**/node_modules/**",
      "!api/**/*.test.js"
    ],
    "coverageDirectory": "coverage",
    "coverageReporters": [
      "text",
      "lcov",
      "html"
    ],
    "testMatch": [
      "**/__tests__/**/*.js",
      "**/?(*.)+(spec|test).js"
    ]
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
